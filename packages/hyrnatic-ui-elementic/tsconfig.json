{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "preserve",
    "strict": false,
    "moduleResolution": "node",
    "sourceMap": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": false,
    "declarationDir": "dist",
    "baseUrl": ".",
    "paths": {
      /*"@*//*": [
        "src*//*"
      ],*/
    }
  },
  "include": [
        "src/index.ts",
//    "src/**/*.ts",
//    "src/**/*.tsx",
//    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ]
}
